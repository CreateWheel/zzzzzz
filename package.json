{
  "name": "zzzzzz",
  "version": "0.1.0",
  "packageManager": "pnpm@8.6.1",
  "author": "Ray <i@mk1.io> (@so1ve)",
  "description": "Sleep function for js.",
  "keywords": [
    "sleep"
  ],
  "homepage": "https://github.com/so1ve/zzzzzz#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/so1ve/zzzzzz.git"
  },
  "bugs": {
    "url": "https://github.com/so1ve/zzzzzz/issues"
  },
  "license": "MIT",
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.cjs",
      "import": "./dist/index.mjs"
    },
    "./worker": {
      "types": "./dist/worker.d.ts",
      "require": "./dist/worker.cjs",
      "import": "./dist/worker.mjs"
    }
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*",
        "./dist/index.d.ts"
      ]
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "rimraf dist && puild",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "prepublishOnly": "nr build",
    "release": "bumpp --commit --push --tag && pnpm publish",
    "start": "tsx src/index.ts",
    "test": "vitest",
    "typecheck": "tsc --noEmit",
    "watch": "puild --watch"
  },
  "devDependencies": {
    "@antfu/ni": "^0.21.0",
    "@babel/types": "^7.18.10",
    "@so1ve/eslint-config": "^0.99.0",
    "@types/node": "^18.7.6",
    "bumpp": "^9.0.0",
    "eslint": "^8.22.0",
    "jiti": "^1.14.0",
    "pnpm": "^8.0.0",
    "puild": "^1.2.3",
    "rimraf": "^3.0.2",
    "tsx": "^3.8.2",
    "typescript": "^5.0.0",
    "vite": "^4.0.0",
    "vitest": "^0.32.0"
  }
}
